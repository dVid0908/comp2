def potencias_5(num):
    suma = 0
    resultado = False
    
    for i in str(num):
        suma += i**5
        
    if suma == num:
        resultado = True
        
    return resultado

lista_result = []
for i in range(1,100000):
    if potencias_5(i):
        lista_result.append(i)
        
print(lista_result)
